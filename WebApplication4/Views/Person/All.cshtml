@model List<Person>
@{
    ViewData["Title"] = "All persons";
}
<div>
    <hr />
    @{
        foreach (Person p in Model){
             <dl class="row">
        <dt class = "col-sm-2">
            id
        </dt>
        <dd class = "col-sm-10">
            @p.id
        </dd>
        <dt class = "col-sm-2">
            first_name
        </dt>
        <dd class = "col-sm-10">
          @p.first_name
        </dd>
        <dt class = "col-sm-2">
           last_name
        </dt>
        <dd class = "col-sm-10">
            @p.last_name
        </dd>
        <dt class = "col-sm-2">
            email
        </dt>
        <dd class = "col-sm-10">
                    @p.email
        </dd>
        <dt class = "col-sm-2">
            image
        </dt>
        <dd class = "col-sm-10">
                    @p.image
        </dd>
        <dt class = "col-sm-2">
           country
        </dt>
        <dd class = "col-sm-10">
            @p.country
        </dd>
    </dl>
        }
    }
   
</div>